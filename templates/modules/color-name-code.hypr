<style text="text/css">
{% for colors in themeSettings.colorNameAndColorHexCodeArray %}
    {% if colorName.multi|lower != "multi" %}
        .{{ colors.colorName|lower }}, .{{ colors.colorName|capfirst }} {
            background-color: {{ colors.colorValue }};
        }
    {% else %}
        .{{ colors.colorName|lower }}, .{{ colors.colorName|capfirst }} {
            background: {{ colors.colorValue }};
        }
    {% endif %}
{% endfor %}
</style>

