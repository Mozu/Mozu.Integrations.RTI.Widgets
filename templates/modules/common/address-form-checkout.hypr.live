                            <md-option value="ID" {% if model.address.stateOrProvince == 'ID' %}selected="selected"{% endif %}>ID</md-option>
                            <md-option value="IL" {% if model.address.stateOrProvince == 'IL' %}selected="selected"{% endif %}>IL</md-option>
                            <md-option value="IN" {% if model.address.stateOrProvince == 'IN' %}selected="selected"{% endif %}>IN</md-option>
