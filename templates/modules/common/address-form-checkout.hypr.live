{% block address-form %}
    <div class="row completed-address-container">
        <div class="col-xs-24">
            <span class="address-line">
                {{ model.firstName }} {{ model.lastNameOrSurname }}
            </span>
            <span class="address-line">
                {{ model.address.address1 }}
            </span>
            <span class="address-line">
              {{ model.address.address2 }}
            </span>
            <span class="address-line">
              {{ model.address.cityOrTown }}, {{ model.address.stateOrProvince }} {{ model.address.postalOrZipCode }}
            </span>
            <span class="address-line">
              {{ model.phoneNumbers.home }}
            </span>
        </div>
    </div>
    <div class="row address-form-container hide-loading-el">
         <div class="col-xs-24 col-sm-24 col-md-11 col-lg-11">
             <md-input-container flex>
                 <label for="firstname">{{ labels.firstName }}</label>
                 <input type="text" class="input-padding-lg" name="firstname" data-mz-value="{{ addresscontext }}firstName" value="{{ model.firstName }}" maxlength="200">
             </md-input-container>
             <span class="mz-validationmessage" data-mz-validationmessage-for="{{ addresscontext }}firstName"></span>
        </div>
        <div class="col-xs-24 col-sm-24 col-md-11 col-lg-11 col-lg-offset-2 col-md-offset-2">
            <md-input-container flex>
                <label for="lastname">{{ labels.lastName }}</label>
                <input type="text" class="input-padding-lg chkout-first-name" name="lastname" data-mz-value="{{ addresscontext }}lastNameOrSurname" value="{{ model.lastNameOrSurname }}" maxlength="200">
            </md-input-container>
            <span class="mz-validationmessage" data-mz-validationmessage-for="{{ addresscontext }}lastNameOrSurname"></span>
        </div>
    </div>

    <div class="row mz-addressform-address1 address-form-container hide-loading-el">
        <md-input-container flex>
            <label for="address-line-1">{{ labels.address }}</label>
            <input type="text" class="input-padding-mid chkout-last-name" name="address-line-1" data-mz-value="{{ addresscontext }}address.address1" value="{{ model.address.address1 }}" maxlength="200">
        </md-input-container>
        <span class="mz-validationmessage" data-mz-validationmessage-for="{{ addresscontext }}address.address1"></span>
    </div>

    <div class="row mz-addressform-address2 address-form-container hide-loading-el">
        <md-input-container flex>
            <label for="address-line-1">{{ labels.address2 }}</label>
            <input type="text" class="input-padding-mid" name="address-line-2" data-mz-value="{{ addresscontext }}address.address2" value="{{ model.address.address2 }}" maxlength="200">
        </md-input-container>
        <span class="mz-validationmessage"></span>
    </div>


    <div class="mz-addressform-country address-form-container hide-loading-el" ng-hide="true">
        <div class="">
            <label for="country">{{ labels.country }}<span class="is-required"> *</span></label>
        </div>
        <div class="countryDrop">
            <select data-mz-value="{{ addresscontext }}address.countryCode">
                <option></option>
                {% if countries && countries.length > 0 %}
                {% for country in countries %}
                    <option {% if model.address.countryCode == country.value %}selected="selected" {% endif %}value="{{ country.value }}">{{ country.key }}</option>
                {% endfor %}
                {% else %}
                {% for country in pageContext.billingCountries %}
                    <option {% if model.address.countryCode == country.value %}selected="selected" {% endif %}value="{{ country.value }}">{{ country.key }}</option>
                {% endfor %}
                {% endif %}
            </select>
            <span class="mz-validationmessage" data-mz-validationmessage-for="{{ addresscontext }}address.countryCode"></span>
        </div>
    </div>
    <div class="row address-form-container hide-loading-el">
        <div class="col-xs-24 col-sm-24 col-md-12 col-lg-12">
            <md-input-container flex>
                <label for="city">{{ labels.city }}</label>
                <input type="text" class="input-padding-lg" name="city" data-mz-value="{{ addresscontext }}address.cityOrTown" value="{{ model.address.cityOrTown }}" maxlength="100">
            </md-input-container>
            <span class="mz-validationmessage" data-mz-validationmessage-for="{{ addresscontext }}address.cityOrTown"></span>


            <!--<div class="mz-addressform-city">
                <div class="row">
                    <label for="city">{{ labels.city }}<span class="is-required"> *</span></label>
                </div>
                <div class="row">
                    <input type="text" class="input-padding-lg" name="city" data-mz-value="{{ addresscontext }}address.cityOrTown" value="{{ model.address.cityOrTown }}" maxlength="100">
                    <span class="mz-validationmessage" data-mz-validationmessage-for="{{ addresscontext }}address.cityOrTown"></span>
                </div>
                <div class="mz-addressform-autocompletefields">
                    <label for="autocomplete-zip">{{ labels.zip }}</label><input type="text" class="input-padding-mid" name="zip" data-mz-value="{{ addresscontext }}address.postalOrZipCode" value="{{ model.address.postalOrZipCode }}" />
                </div>
            </div>-->

        </div>
        <div class="col-xs-24 col-sm-24 col-md-5 col-lg-5 col-lg-offset-1 col-md-offset-1">
            <div class="checkout-address-state">
                <label for="autocomplete-state" ng-hide="true">{{ labels.state }}</label>
                {% if model.address.countryCode != "US" %}
                    <input type="text" class="input-padding-lg chkout-state-inputfield" name="state" data-mz-value="{{ addresscontext }}address.stateOrProvince" value="{{ model.address.stateOrProvince }}" />
                {% else %}
                    <select id="shipto-state" data-mz-value="{{ addresscontext }}address.stateOrProvince" class="us-states" ng-hide="true">
                        <option value="">{{ labels.selectState }}</option>
                        {% for state in themeSettings.usStates %}
                            <option {% if model.address.stateOrProvince == state.abbreviation %}selected="selected"{% endif %} value="{{ state.abbreviation }}">{{ state.name }}</option>
                        {% endfor %}
                    </select>
