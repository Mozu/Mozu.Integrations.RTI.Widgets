                            <md-option value="MD" {% if model.address.stateOrProvince == 'MD' %}selected="selected"{% endif %}>MD</md-option>
                            <md-option value="MA" {% if model.address.stateOrProvince == 'MA' %}selected="selected"{% endif %}>MA</md-option>
                            <md-option value="MI" {% if model.address.stateOrProvince == 'MI' %}selected="selected"{% endif %}>MI</md-option>
                            <md-option value="MN" {% if model.address.stateOrProvince == 'MN' %}selected="selected"{% endif %}>MN</md-option>
                            <md-option value="MS" {% if model.address.stateOrProvince == 'MS' %}selected="selected"{% endif %}>MS</md-option>
