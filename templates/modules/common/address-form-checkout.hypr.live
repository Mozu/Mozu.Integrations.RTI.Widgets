                            <md-option value="MP" {% if model.address.stateOrProvince == 'MP' %}selected="selected"{% endif %}>MP</md-option>
                            <md-option value="OH" {% if model.address.stateOrProvince == 'OH' %}selected="selected"{% endif %}>OH</md-option>
                            <md-option value="OK" {% if model.address.stateOrProvince == 'OK' %}selected="selected"{% endif %}>OK</md-option>
                            <md-option value="OR" {% if model.address.stateOrProvince == 'OR' %}selected="selected"{% endif %}>OR</md-option>
                            <md-option value="PW" {% if model.address.stateOrProvince == 'PW' %}selected="selected"{% endif %}>PW</md-option>
                            <md-option value="PA" {% if model.address.stateOrProvince == 'PA' %}selected="selected"{% endif %}>PA</md-option>
                            <md-option value="PR" {% if model.address.stateOrProvince == 'PR' %}selected="selected"{% endif %}>PR</md-option>
                            <md-option value="RI" {% if model.address.stateOrProvince == 'RI' %}selected="selected"{% endif %}>RI</md-option>
                            <md-option value="SC" {% if model.address.stateOrProvince == 'SC' %}selected="selected"{% endif %}>SC</md-option>
                            <md-option value="SD" {% if model.address.stateOrProvince == 'SD' %}selected="selected"{% endif %}>SD</md-option>
